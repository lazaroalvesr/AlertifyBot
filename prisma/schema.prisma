generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model userName {
  id           String  @id @default(uuid())
  TwitchChannelName         String
  guildId      String  @unique
  DiscordChannelId    String  
  liveNotified Boolean @default(false)
}
